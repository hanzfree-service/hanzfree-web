{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "./src",
    "paths": {
      "@pages/*": ["../pages/*"],
      "@shared/*": ["shared/*"],
      "@admin/*": ["admin/*"],
      "@main/*": ["main/*"],
      "@signUp/*": ["signUp/*"],
      "@reservation/*": ["reservation/*"],
      "@booking/*": ["booking/*"]
    },
    "forceConsistentCasingInFileNames": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "pages/**/*.tsx"],
  "exclude": ["node_modules"]
}
